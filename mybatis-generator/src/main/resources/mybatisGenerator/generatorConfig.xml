<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
	<context id="DB2Tables" targetRuntime="MyBatis3">
		<property name="autoDelimitKeywords" value="true" />
		<property name="beginningDelimiter" value="`" />
		<property name="endingDelimiter" value="`" />

		<plugin type="org.mybatis.generator.plugins.EqualsHashCodePlugin" />
		<plugin type="org.mybatis.generator.plugins.SerializablePlugin" />

		<!-- 注释 -->
		<commentGenerator>
			<property name="suppressAllComments" value="true" /><!-- 是否取消注释 -->
			<property name="suppressDate" value="false" /> <!-- 是否生成注释代时间戳 -->
		</commentGenerator>

		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://192.168.3.113:3306/opt_quote" userId="root"
			password="root" />
		<javaTypeResolver type="com.xlm.mybatisgenerator.plugin.MyJavaTypeResolver">
			<property name="forceBigDecimals" value="false"></property>
		</javaTypeResolver>

		<javaModelGenerator targetPackage="price"
			targetProject="src/main/resources/mybatisGenerator/java">
			<property name="enableSubPackages" value="false" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>

		<sqlMapGenerator targetPackage="price"
			targetProject="src/main/resources/mybatisGenerator/xml">
			<property name="enableSubPackages" value="false" />
		</sqlMapGenerator>

		<javaClientGenerator type="XMLMAPPER"
			targetPackage="price" targetProject="src/main/resources/mybatisGenerator/dao">
			<property name="enableSubPackages" value="false" />
		</javaClientGenerator>

 		<table tableName="t_competitive_price" domainObjectName="CompetitivePrice" enableCountByExample="false"
			   enableDeleteByExample="false" enableSelectByExample="false"
			   enableUpdateByExample="false">
 		</table>

		<table tableName="t_platform_reference_price_approve_result" domainObjectName="PlatformReferencePriceApproveResult" enableCountByExample="false"
			   enableDeleteByExample="false" enableSelectByExample="false"
			   enableUpdateByExample="false">

		</table>
		<table tableName="t_platform_reference_price_warn" domainObjectName="PlatformReferencePriceWarn" enableCountByExample="false"
			   enableDeleteByExample="false" enableSelectByExample="false"
			   enableUpdateByExample="false">
		</table>
		<table tableName="t_platform_reference_price_warn_history" domainObjectName="PlatformReferencePriceWarnHistory" enableCountByExample="false"
			   enableDeleteByExample="false" enableSelectByExample="false"
			   enableUpdateByExample="false">
		</table>
		<table tableName="t_reference_price" domainObjectName="ReferencePrice" enableCountByExample="false"
			   enableDeleteByExample="false" enableSelectByExample="false"
			   enableUpdateByExample="false">
		</table>
		<table tableName="t_reference_price_trace" domainObjectName="ReferencePriceTrace" enableCountByExample="false"
			   enableDeleteByExample="false" enableSelectByExample="false"
			   enableUpdateByExample="false">
		</table>
	</context>
</generatorConfiguration>